                                                        alert("I told you...");
                                                        $("#factIn").val("");

                                                    }

                                                    //Step2: Calculate the Factorial
                                                    for (var loop = number - 1; loop > 1; loop--) {
                                                        //How do we claculate the factorial
                                                        //numer=number*loop
                                                        number *= loop;
                                                    }

                                                    //Step 3: Show the results
